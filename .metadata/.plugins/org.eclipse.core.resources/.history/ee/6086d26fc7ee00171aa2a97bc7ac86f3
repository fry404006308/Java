package com.fry.poiDemo.bean;

import java.util.List;

public class Option {
	String text;//选项中的文字
	List<Image_> images;//选项中用到的图片
	int questionNum;
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public List<Image_> getImages() {
		return images;
	}
	public void setImages(List<Image_> images) {
		this.images = images;
	}
	
	
	
}
