package com.fry.poiDemo.bean;

public class Option {
	String text;//选项中的文字
	Image_ images[];//选项中用到的图片
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public Image_[] getImages() {
		return images;
	}
	public void setImages(Image_[] images) {
		this.images = images;
	}
	
	
}
